{
  "chains": [
    {
      "switch": "r1",
      "table": "MyIngress.serviceChainIngress",
      "rules": [
        {
          "match": { "hdr.ipv4.dstAddr": ["10.0.2.0", 24] },
          "action": "MyIngress.to_vlb",
          "params": { "port": 4 }
        }
      ]
    },
    {
      "switch": "r4",
      "table": "MyIngress.serviceChainEgress",
      "rules": [
        {
          "match": { "hdr.ipv4.dstAddr": ["10.0.2.0", 24] },
          "action": "MyIngress.to_vmon",
          "params": { "port": 4 }
        }
      ]
    }
  ]
}
  